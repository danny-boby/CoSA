INPUT
 input: BV(8);

OUTPUT
 output: BV(8);

VAR
 clk: BV(1);
 max: BV(8);

 fifo_1: FIFO(input, output, clk);

INVAR
max = 6_8;

INIT
clk = 0_1;

TRANS
(clk = 0_1) <-> (next(clk) = 1_1);


DEF FIFO(in: BV(8), out: BV(8), clk: BV(1)):
  STATE
  cell_0: BV(8);
  cell_1: BV(8);
  cell_2: BV(8);
  cell_3: BV(8);
  cell_4: BV(8);
  cell_5: BV(8);

  INVAR
  out = cell_5;

  TRANS
  ((clk = 0_1) & (next(clk) = 1_1)) -> (next(cell_0) = in) & (next(cell_1) = cell_0) & (next(cell_2) = cell_1)  & (next(cell_3) = cell_2) & (next(cell_4) = cell_3) & (next(cell_5) = cell_4);
  (!((clk = 0_1) & (next(clk) = 1_1))) -> (next(cell_0) = cell_0) & (next(cell_1) = cell_1) & (next(cell_2) = cell_2)  & (next(cell_3) = cell_3) & (next(cell_4) = cell_4) & (next(cell_5) = cell_5);
  